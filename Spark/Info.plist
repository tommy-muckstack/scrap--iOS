    <true/>
    <key>UIAppFonts</key>
    <array>
        <string>Hadley.otf</string>
        <string>Office Notes.otf</string>
        <string>Office Notes.ttf</string>
        <string>Satoshi-Regular.otf</string>
        <string>Satoshi-Medium.otf</string>
        <string>Satoshi-Bold.otf</string>
        <string>Satoshi-Variable.ttf</string>
    </array>
