            <array>
                <string>com.googleusercontent.apps.520567341698-h372oqpma77ge4o0v42pssnh20iesk91</string>
            </array>
        </dict>
        <dict>
            <key>CFBundleURLName</key>
            <string>BundleID</string>
            <key>CFBundleURLSchemes</key>
            <array>
                <string>com.muckstack.spark</string>
            </array>
        </dict>
    </array>
